{
  "name": "saded",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:webpack": "next dev --webpack",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:low-resource": "NODE_OPTIONS='--max-old-space-size=2048' NEXT_PRIVATE_SKIP_TURBO=1 next build",
    "build:push": "npm run build && git add -f .next/server/ .next/static/ .next/BUILD_ID .next/*.json .next/*.js && git commit -m \"Build: Update .next files - $(date +'%Y-%m-%d %H:%M:%S')\" && git push origin $(git branch --show-current)",
    "start": "node server.js",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate-postgres": "tsx scripts/migrate-json-to-postgres.ts",
    "setup-postgres": "node scripts/setup-postgres.js",
    "setup-postgres-retry": "node scripts/setup-postgres-with-retry.js",
    "setup-db-production": "node scripts/setup-db-production.js",
    "setup-mysql": "node scripts/setup-mysql.js",
    "create-vehicles-table": "node scripts/create-vehicles-table-manual.js",
    "fix-vehicle-logo-column": "node scripts/fix-vehicle-logo-column.js",
    "test-mysql": "node scripts/test-mysql-connection.js",
    "test-vehicle-logo": "node scripts/test-vehicle-logo.js",
    "install-mysql": "powershell -ExecutionPolicy Bypass -File scripts/install-mysql-windows.ps1",
    "install-xampp": "powershell -ExecutionPolicy Bypass -File scripts/install-xampp-windows.ps1",
    "check-deployment": "node scripts/check-deployment.js",
    "health-check": "node scripts/health-check.js",
    "seed-db": "node scripts/seed-database.js",
    "create-indexes": "mysql -u $DB_USER -p$DB_PASSWORD $DB_NAME < scripts/create-db-indexes.sql"
  },
